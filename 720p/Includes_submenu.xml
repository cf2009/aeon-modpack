<includes>
<include name="homesubmenu_animation">
	<animation type="Conditional" condition="Skin.HasSetting(submenu)" reversible="false">
		<effect type="fade" start="0" end="100" time="350" delay="0" />
		<effect type="zoom" start="20" end="100" center="auto" time="350" delay="0" tween="Cubic" asing="inout" />
	</animation>
	<animation type="Hidden" reversible="false">
		<effect type="fade" start="100" end="0" time="350" delay="0" />
		<effect type="zoom" start="100" end="20" center="auto" time="350" delay="0" tween="Cubic" asing="inout" />
	</animation>
</include>
<include name="homesubmenu_animation2">
	<animation type="Conditional" condition="Skin.HasSetting(submenu)" reversible="false">
		<effect type="zoom" start="100" end="200" center="auto" time="350" delay="0" tween="Cubic" asing="inout" />
		<effect type="fade" start="100" end="0" time="400" delay="0" />
	</animation>
	<animation type="Conditional" condition="!Skin.HasSetting(submenu)" reversible="false">
		<effect type="zoom" start="200" end="100" center="auto" time="350" delay="0" tween="Cubic" asing="inout" />
		<effect type="fade" start="0" end="100" time="350" delay="0" />
	</animation>
</include>
